{
  "swagger": "2.0",
  "info": {
    "title": "Сервис для базы 1C АНИТ",
    "version": "1.2.0",
    "description": "API для получения различных данных\nверсия от 2022-05-17",
    "contact": {
      "email": "ag@79900.ru"
    }
  },
  "basePath": "/copy_erp/hs/oas_api",
  "schemes": [
    "http"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/ping": {
      "get": {
        "summary": "Проверяет, жив ли сервер",
        "description": "Просто возвращает 200. Используется в отладочных и проверочных целях.",
        "operationId": "getPing",
        "responses": {
          "200": {
            "description": "Время на сервере",
            "schema": {
              "$ref": "#/definitions/ResponsePingServer"
            }
          }
        }
      }
    },
    "/catalogs/{type}/{guid}": {
      "get": {
        "description": "Просто возвращает 200. Используется в отладочных и проверочных целях.",
        "operationId": "getCatalogElemByGuid",
        "parameters": [
          {
            "in": "path",
            "name": "type",
            "description": "Тип справочника",
            "type": "string",
            "required": true
          },
          {
            "in": "path",
            "name": "guid",
            "description": "Тип справочника",
            "type": "string",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "Время на сервере",
            "schema": {
              "$ref": "#/definitions/RefToCatalog"
            }
          }
        }
      }
    }
  },
  "security": [
    {
      "basicAuth": []
    }
  ],
  "securityDefinitions": {
    "basicAuth": {
      "type": "basic"
    }
  },
  "definitions": {
    "ResponsePingServer": {
      "description": "Дата и время на сервере",
      "type": "object",
      "properties": {
        "data_time": {
          "type": "string",
          "format": "date-time"
        },
        "current_user": {
          "description": "Текущий пользователь",
          "$ref": "#/definitions/RefToCatalog"
        }
      },
      "required": [
        "data_time",
        "current_user"
      ]
    },
    "RefToCatalog": {
      "description": "Ссылка на справочник",
      "type": "object",
      "properties": {
        "guid": {
          "type": "string",
          "description": "Уникальный идентификатор"
        },
        "type": {
          "type": "string",
          "description": "Тип справочника"
        },
        "code": {
          "type": "string",
          "description": "Код справочника"
        },
        "name": {
          "type": "string",
          "description": "Наименование справочника"
        }
      }
    }
  },
  "externalDocs": {
    "description": "Find out more about Swagger",
    "url": "http://swagger.io"
  }
}